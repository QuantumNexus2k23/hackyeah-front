{
  "expo": {
    "scheme": "acme",
    "splash": {
      "image": "./assets/images/logo.png"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "name": "Paneuropa Guide",
    "slug": "hackyeah-front",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bfc7e615-90cd-458d-b4da-0f652baf546f"
      }
    },
    "android": {
      "package": "com.paneuropa.guide",
      "icon": "./assets/images/logo.png",
      "config": {
        "googleMaps": {
          "apiKey": "api-key"
        }
      }
    },
    "ios": {
      "icon": "./assets/images/logo.png"
    },
    "owner": "quantumnexus"
  }
}
